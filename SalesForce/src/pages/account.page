<apex:page standardController="Account">
 <apex:form >
 <apex:pageBlock >
 <apex:pageBlockSection title="Account Info">
 <apex:inputField id="name" value="{!Account.Name}"/>
     <apex:inputField id="accountno" value="{!Account.AccountNumber}"/>
          <apex:inputField id="Rating" value="{!Account.Rating}"/>
     
 </apex:pageBlockSection>
<apex:pageBlockButtons >
    <apex:commandButton id="save" action="{!save}" value="save"/>
        <apex:commandButton id="cancel" action="{!cancel}" value="cancel"/>
</apex:pageBlockButtons>
 </apex:pageBlock>
 </apex:form>
 
 
</apex:page>